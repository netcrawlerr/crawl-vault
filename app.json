{
  "expo": {
    "name": "Crawl Vault",
    "slug": "password-manager",
    "scheme": "password-manager-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lock.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lock.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.netcrawler.crawl_vault",
      "permissions": ["INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "02181577-5249-4af6-934a-a2a72393d3d4"
      }
    }
  }
}
